package Ex8_5;

import java.io.*;

public class FileStreamEx {
	public static void main(String[] args) {
		byte a[] = {7, 51, 3, 4, -1, 24}; // -128~+127
		
		FileOutputStream fout = null;
		
		try {
			fout = new FileOutputStream("C:\\Java1630\\test.out");
		} catch(IOException e) {
			System.out.println("저장 불가");
		}
	}
}
